    "generate": "nuxt generate",
    "help": "nuxt",
    "pm2": "cross-env NODE_ENV=production pm2-runtime start server/index.js -i 0 --name blog-front"
  },
  "config": {
    "nuxt": {
      "port": 9907,
      "host": "0.0.0.0"
    }
  },

    "hot-shots": "^7.1.0",
    "html-entities": "^2.3.2",
    "ioredis": "^4.16.0",
    "is-number": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "markdown-it": "^11.0.0",
    "markdown-it-highlightjs": "^3.2.0",
    "moment": "^2.24.0",
    "mongodb": "^3.5.5",

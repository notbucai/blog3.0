    "@types/ejs": "^3.0.1",
    "@types/express": "^4.17.1",
    "@types/ioredis": "^4.22.2",
    "@types/is-number": "^7.0.1",
    "@types/jest": "^24.0.18",
    "@types/jsonwebtoken": "^8.3.8",
